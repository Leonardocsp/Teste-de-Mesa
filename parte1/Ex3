using System;

namespace TesteDeMesa
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a, b;

            a = 7;

            b = a - 6;
            
            int[] v = new int[a];

            while (b < a)
            {
                v[b] = b + a;

                Console.WriteLine("v[{0}] = {1}", b, v[b]);

                b = b + 2;
            }
            Console.ReadKey();
        }
    }
}
