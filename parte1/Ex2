using System;

namespace TesteDeMesa
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a;
            int[] v = new int[6];

            a = 2;

            while (a < 6)
            {
                v[a] = 10 * a;
                Console.WriteLine("v[{0}] = {1}", a, v[a]);

                a += 1;
            }

            Console.ReadKey();
        }
    }
}
